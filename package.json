{
  "name": "@armathai/lego",
  "version": "2.0.0",
  "description": "Simple, scalable state management",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "private": false,
  "scripts": {
    "test": "jest --config jestconfig.json",
    "start": "npx parcel src/__examples__/index.html",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build --scripts-prepend-node-path",
    "prepublishOnly": "npm test && npm run lint",
    "generate-event-names": "node src/__examples__/scripts/generate-event-names.js --name=ModelEvents --input=./src/__examples__/ts/model --output=./src/__examples__/ts/events --type=ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caki0915/my-awesome-greeter.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "MVC"
  ],
  "author": "Armathai LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caki0915/my-awesome-greeter/issues"
  },
  "homepage": "https://github.com/caki0915/my-awesome-greeter#readme",
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "bufferutil": "^4.0.1",
    "canvas": "^2.6.1",
    "codecov": "^3.6.5",
    "jest": "^25.1.0",
    "minimist": "^1.2.0",
    "parcel-bundler": "^1.12.4",
    "phaser": "^3.22.0",
    "prettier": "^1.18.2",
    "ts-jest": "^25.2.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5",
    "utf-8-validate": "^5.0.2"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {}
}
